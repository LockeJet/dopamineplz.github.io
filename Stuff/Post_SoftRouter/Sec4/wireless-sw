
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'HT20'
	option txpower '21'
	option country 'US'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Verizon'
	option encryption 'psk-mixed'
	option key ''

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'verizon2'
	option network 'lan2'
	option encryption 'psk-mixed'
	option key ''

