	option exec '/etc/init.d/mwan3 reload'

config adbyby
	option init 'adbyby'

config ipsec
	option init 'ipsec'

config vlmcsd
	option init 'kms'

config arpbind
	option init 'arpbind'

config frp
	option init 'frp'

